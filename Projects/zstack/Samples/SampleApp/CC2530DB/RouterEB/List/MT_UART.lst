    223          /***************************************************************************************************
    224           * @fn      MT_UartProcessZAppData
    225           *
    226           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    227           *          |  1  |  2   |       1         |  1   |
    228           *
    229           *          Parses the data and determine either is SPI or just simply serial data
    230           *          then send the data to correct place (MT or APP)
    231           *
    232           * @param   port    - UART port
    233           *          event   - Event that causes the callback
    234           *
    235           *
    236           * @return  None
    237           ***************************************************************************************************/
    238          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    239          {
    240          
    241            osal_event_hdr_t  *msg_ptr;
    242            uint16 length = 0;
    243            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    244          
    245            /*
    246               If maxZAppBufferLength is 0 or larger than current length
    247               the entire length of the current buffer is returned.
    248            */
    249            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    250            {
    251              length = MT_UartMaxZAppBufLen;
    252            }
    253            else
    254            {
    255              length = rxBufLen;
    256            }
    257          
    258            /* Verify events */
    259            if (event == HAL_UART_TX_FULL)
    260            {
    261              // Do something when TX if full
    262              return;
    263            }
    264          
    265            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    266            {
    267              if ( App_TaskID )
    268              {
    269                /*
    270                   If Application is ready to receive and there is something
    271                   in the Rx buffer then send it up
    272                */
    273                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    274                {
    275                  /* Disable App flow control until it processes the current data */
    276                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    277          
    278                  /* 2 more bytes are added, 1 for CMD type, other for length */
    279                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    280                  if ( msg_ptr )
    281                  {
    282                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    283                    msg_ptr->status = length;
    284          
    285                    /* Read the data of Rx buffer */
    286                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    287          
    288                    /* Send the raw data to application...or where ever */
    289                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    290                  }
    291                }
    292              }
    293            }
    294          }
